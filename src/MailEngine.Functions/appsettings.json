{
  "DatabaseProvider": "PostgreSQL",
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}"
  },
  "AzureServiceBus": {
    "ConnectionString": "${SERVICEBUS_CONNECTION_STRING}"
  },
  "KeyVault": {
    "VaultUri": "${KEYVAULT_URI}"
  },
  "Providers": {
    "Concurrency": {
      "MaxPerProvider": 10
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
